{
    "eula": "",
    "vendor": "IFM",
    "icon": "https://1dawu2.github.io/ifm-wiz/icon.png",
    "license": "MIT",
    "id": "ifm.drag",
    "version": "0.0.1",
    "name": "IFM Drag&Drop",
    "newInstancePrefix": "IFM",
    "description": "IFM Drag&Drop List",
    "imports": [],
    "webcomponents": [
        {
            "kind": "main",
            "tag": "ifm-drag",
            "url": "https://1dawu2.github.io/ifm-wiz/dist/main_bundle.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "listOfItems": {
            "type": "string[]",
            "description": "List Items",
            "default": [
                "Prod1",
                "Prod2"
            ]
        }
    },
    "dataBindings": {},
    "methods": {
        "setValue": {
            "description": "set the list items",
            "parameters": [
                {
                    "name": "newItem",
                    "type": "string",
                    "description": "new List Item"
                }
            ],
            "body": "var list = this.listOfItems; list.push(newItem); this.listOfItems = list;"
        },
        "getValue": {
            "returnType": "string[]",
            "description": "return List Items",
            "body": "return this.listOfItems;"
        }
    },
    "events": {
        "onStart": {
            "description": "onStart events trigger"
        }
    }
}